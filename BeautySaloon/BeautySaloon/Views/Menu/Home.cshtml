    <!-- POPUP -->
    <div id="online" class="modalLogin" style="height: 100%;padding-top: 100px;">
        <!-- Modal Content -->
        <form class="modal-content animate" asp-action="CreateOrder" method="post">
            <div class="containerLogin">
                <h3 class="title">Онлайн запись<span></span></h3>
                <label class="text">Выберите дату: </label>
                <p><input asp-for="Date" type="date" required=""></p>

                <label class="text">Выберите мастера: </label>
                <div>
                    <select class="view" asp-for="MasterID" asp-items="ViewBag.Masters"></select>
                </div>

                <label class="text">Выберите услугу: </label>
                <div>
                    <select class="view" asp-for="ServiceID" asp-items="ViewBag.Services"></select>
                </div>

                <button class="OK" type="submit">Записаться</button>
            </div>

            <div class="containerLogin" style="background-color:#f1f1f1">
                <a class="cancelbtn" asp-action="Home">Cancel</a>
            </div>
        </form>
    </div>


    <!-- POPUP -->
    <!-- POPUP -->
    <!-- The Modal -->


    <script>
        // Get the modal
        var modal = document.getElementById('login');

        // When the user clicks anywhere outside of the modal, close it
        window.onclick = function (event) {
            if (event.target == modal) {
                modal.style.display = "none";
            }
        }
    </script>
    <!-- POPUP -->
    <!--//banner bottom-->
    <div id="about" class="feature all_pad wthree">
        <div class="container">
